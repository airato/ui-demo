    "playground": "graphql playground",
    "build": "rimraf dist && tsc"
  },
  "dependencies": {
    "auth0": "^2.9.1",
    "bcryptjs": "^2.4.3",
    "express-jwt": "^5.3.0",
    "express-jwt-authz": "^1.0.0",
    "graphql": "^0.12.3",
    "graphql-yoga": "^1.2.0",
    "prisma-binding": "^1.3.8",
    "jsonwebtoken": "^8.1.0",
    "jwks-rsa": "^1.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "dotenv-cli": "^1.4.0",
    "graphql-cli": "^2.12.4",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2",
    "prisma-cli": "^1.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  }
}
